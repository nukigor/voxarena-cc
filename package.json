{
  "name": "voxarena-cc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "restart-dev": "bash scripts/restart-dev.sh",
    "kill-dev": "pkill -9 -f 'next dev' 2>/dev/null || true; pkill -9 node 2>/dev/null || true",
    "check-servers": "bash scripts/check-dev-health.sh",
    "db:setup": "./scripts/setup-db.sh",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "db:reset": "docker-compose down -v && docker-compose up -d && npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/lib-storage": "^3.913.0",
    "@elevenlabs/elevenlabs-js": "^2.19.0",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.17.1",
    "@tanstack/react-query": "^5.90.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "dotenv": "^17.2.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.546.0",
    "marked": "^16.4.1",
    "next": "^15.5.6",
    "openai": "^6.5.0",
    "puppeteer": "^24.27.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
